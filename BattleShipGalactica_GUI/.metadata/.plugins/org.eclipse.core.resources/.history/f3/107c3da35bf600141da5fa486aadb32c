package Main;

import javax.swing.JFrame;

public class Main_View extends JFrame{
	
	JFrame frame; 
			
	public Main_View(){
		super();
		this.frame = new JFrame("BattleShip Galactica");
		this.frame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		this.frame.pack();
		this.frame.setVisible(true);
	}
	
	
	
}
